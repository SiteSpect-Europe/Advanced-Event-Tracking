{
  "name": "advanced-event-tracking",
  "version": "1.0.11",
  "description": "Flexible &amp; Advanced Event Tracking for SiteSpect",
  "main": "tracking.js",
  "dependencies": {
    "uglify-js": "^3.7.6"
  },
  "devDependencies": {},
  "scripts": {
    "build": "uglifyjs --compress --mangle --output tracking.ugly.js -- tracking.js && node _/deploy.js dist/tracking.min.js",
    "build-elinview": "uglifyjs --compress --mangle --output tracking.ugly.js -- optional_modules/element-in-view.js && node _/deploy.js dist/tracking.module.element_in_view.min.js",
    "build-spaadapter": "uglifyjs --compress --mangle --output tracking.ugly.js -- optional_modules/spa-adapter.js && node _/deploy.js  dist/tracking.module.spa-adapter.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiteSpect-Europe/Advanced-Event-Tracking.git"
  },
